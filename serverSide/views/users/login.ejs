<% layout('layouts/boilerplate')%>
<h1>Login</h1>
<form action="/api/users/login" method="post">
  <input type="text" placeholder="username" name="username" required />
  <input
    type="password"
    placeholder="Enter Password"
    name="userPass"
    required
  />
  <button>Login</button>
</form>
