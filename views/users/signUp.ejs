<% layout('layouts/boilerplate')%>
    <h1>SignUp</h1>
    <form action="/signUp" method="post">
        <input type="text" placeholder="Full Name" name="name" required>
        <input type="text" placeholder="username" name="username" required>
        <input type="email" name="email" placeholder="Enter your emailId" required>
        <input type="text" name="phone_no" placeholder="Mobile number" required>
        <input type="password" placeholder="Create Password" name="password" required>
        <input type="password" placeholder="Confirm Password" name="con_password" required>
        <button>SignUp</button>
    </form>